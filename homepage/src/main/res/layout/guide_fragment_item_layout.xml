<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/home_item_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#cccccc"
        android:layout_marginTop="@dimen/channel_title_margin_top"
        android:layout_marginBottom="@dimen/channel_title_margin_bottom"
        android:layout_marginLeft="@dimen/channel_title_margin_left"
        android:layout_marginRight="@dimen/channel_title_margin_right"
        android:textSize="@dimen/channel_title_size"/>

    <include
        android:id="@+id/banner_guide"
        layout="@layout/banner_guide"/>

    <include
        android:id="@+id/banner_order"
        layout="@layout/banner_order"/>

    <include
        android:id="@+id/banner_movie"
        layout="@layout/banner_movie"/>

    <include
        android:id="@+id/banner_tv_play"
        layout="@layout/banner_tv_player"/>

    <include
        android:id="@+id/banner_519"
        layout="@layout/banner_519"/>

    <include
        android:id="@+id/banner_conlumn"
        layout="@layout/banner_conlumn"/>

    <include
        android:id="@+id/banner_big_small_ld"
        layout="@layout/banner_big_small_ld"/>

    <include
        android:id="@+id/banner_double_md"
        layout="@layout/banner_double_md"/>

    <include
        android:id="@+id/banner_double_ld"
        layout="@layout/banner_double_ld"/>
</LinearLayout>